{
  "name": "@frontiernav/data",
  "version": "1.0.0",
  "description": "Data for FrontierNav",
  "scripts": {
    "preinstall": "node ./scripts/enforceYarn.js",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "jest",
    "lint": "eslint \"packages/**/*.js\"",
    "deps": "yarn upgrade-interactive --latest && yarn upgrade && git add yarn.lock '*package.json' && git commit -m 'build(deps): upgrade dependencies'"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "extractors/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jahed/frontiernav-data.git"
  },
  "author": "Jahed Ahmed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jahed/frontiernav-data/issues"
  },
  "homepage": "https://github.com/jahed/frontiernav-data#readme",
  "dependencies": {
    "@frontiernav/filesystem": "1.0.0",
    "@frontiernav/graph": "1.0.0",
    "@frontiernav/spreadsheets": "1.0.0",
    "bluebird": "3.5.2",
    "colors": "1.3.2",
    "csv": "4.0.0",
    "file": "0.2.2",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.11",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "ajv": "6.5.4",
    "eslint": "5.8.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "jest": "23.6.0"
  }
}
